<odoo>
    <record id="helpdesk_ticket_view_form_inherit_oriens_forms" model="ir.ui.view">
        <field name='name'>helpdesk.ticket.form.inherit.oriens.forms</field>
        <field name="model">helpdesk.ticket</field>
        <field name="inherit_id" ref="helpdesk.helpdesk_ticket_view_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='partner_id']" position="after">
                <field name="contact_method" readonly="1" />
                <field name="order_string" readonly="1" />
            </xpath>
        </field>
    </record>
</odoo>